{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "mtunnel-ui",
  "version": "0.1.0",
  "identifier": "one.muisnowdevs.apps.mtunnelui",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Minimal Tunnel",
        "width": 400,
        "height": 600,
        "resizable": false,
        "maximizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": ["binaries/tunnel"]
  }
}
